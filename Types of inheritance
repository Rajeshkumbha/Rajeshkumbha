class Animal
{
    void eat(){
        System.out.println("eating...");}
    }
class Dog extends Animal{
    void bark(){
        System.out.println("barkint...");}
    }
class BadyDog extends Dog{
    void weep(){
        System.out.println("weeping...");}
    }
class Main{
    public static void main(String args[])
    {
        BadyDog d=new BadyDog();
        d.weep();
        d.bark();
        d.eat();
    }
}
